{
    "semi": false,
    "singleQuote":true,
    "printWidth": 80,
    "tabWidth": 2,
     "jsxSingleQuote": false,
     "trailingComma": "all",
     "bracketSpacing": true,
     "jsxBracketSameLine": false,
     "arrowParens": "always",
     "rangeStart": 0,
     "requirePragma": false,
     "insertPragma": false,
     "proseWrap": "preserve",
     "htmlWhitespaceSensitivity": "ignore",
     "endOfLine": "auto",
    "plugins": ["prettier-plugin-vue"]
  }
  